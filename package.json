{
	"name": "bigfeetwebserver",
	"version": "1.0.0",
	"description": "The dedicated (Node.js and Express.js) REST API backend web server for Big Feet Coquitlam's reservation system.",
	"engines": {
		"node": "16.16.0",
		"npm": "9.6.0"
	},
	"main": "dist/index.js",
	"scripts": {
		"create-migration": "typeorm-ts-node-commonjs migration:create",
		"generate-migration": "typeorm-ts-node-commonjs -d src/config/orm.config.ts migration:generate",
		"run-migration": "typeorm-ts-node-commonjs -d src/config/orm.config.ts migration:run",
		"dev": "ts-node-dev --respawn --pretty --transpile-only src/app.ts",
		"build": "tsc",
		"start": "node dist/app.js",
		"postinstall": "npm run build && npm run run-migration"
	},
	"keywords": [],
	"author": "William Gong",
	"license": "ISC",
	"dependencies": {
		"@joi/date": "^2.1.0",
		"@types/morgan": "^1.9.4",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.2",
		"celebrate": "^15.0.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"helmet": "^7.0.0",
		"joi": "^17.9.2",
		"jsonwebtoken": "^9.0.0",
		"morgan": "^1.10.0",
		"mysql": "^2.18.1",
		"pusher": "^5.2.0",
		"rate-limiter-flexible": "^2.4.1",
		"semver": "^7.5.3",
		"ts-node": "^10.9.2",
		"typeorm": "^0.3.20",
		"winston": "^3.9.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/body-parser": "^1.19.2",
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.13",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.17",
		"@types/helmet": "^4.0.0",
		"@types/jest": "^29.5.2",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/mysql": "^2.15.21",
		"@types/node": "^20.3.1",
		"@types/supertest": "^2.0.12",
		"jest": "^29.5.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.1.3"
	}
}
